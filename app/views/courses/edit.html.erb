<%= @course.name %><br>
<%= @course.id %>
<%= form_with model: @course do |form| %>
  <%= form.text_field :name,  placeholder: "Course Name"%>
  <%= form.text_field :hours,  placeholder: "Hours"%>

  <%= form.submit %>
  <%= link_to "Delete", @course, method: :delete, data: { confirm: "You sure?" } %>
<% end %>